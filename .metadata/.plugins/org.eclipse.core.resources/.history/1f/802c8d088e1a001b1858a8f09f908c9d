package edu.umb.cs680.hw05;

public class Car {
	
	private String make, model;
	private int mileage, year;
	private float price;
	
	public Car(String make, String model, int price, int mileage, int year) {
        this.make = make;
        this.model = model; 
        this.price = price;
        this.mileage = mileage;
        this.year = year;
    }
}
